--Insert
call keytype_insert('IDENTITY',false);
call keytype_insert('INTEGER',false);
call keytype_insert('DOUBLE',false);
call keytype_insert('VARCHAR',true);
call keytype_insert('BOOLEAN',false);
call keytype_insert('BINARY',true);
call keytype_insert('DATE',false);
call keytype_insert('TIME',false);
call keytype_insert('TIMESTAMP',false);

--
call relationshiptype_insert('Identifying');
call relationshiptype_insert('Non-Identifying');
--
call databaseschema_insert('Schema1');
--
WbVarDef schemaname="'Schema1'";
WbVarDef schemaid=@"call getSchemaId($[schemaname])";
--
call databasetable_insert ($[schemaid],'Table1');
call databasetable_insert ($[schemaid],'Table2');
--
WbVarDef table1name="'Table1'";
WbVarDef table2name="'Table2'";
WbVarDef table1id=@"call getTableId($[schemaid],$[table1name])";
WbVarDef table2id=@"call getTableId($[schemaid],$[table2name])";
--
call tablekey_insert($[schemaid],$[table1id],'TableId','TableId',true,getKeyTypeId('IDENTITY'),0,1);
call tablekey_insert($[schemaid],$[table1id],'Name','Name',false,getKeyTypeId('VARCHAR'),300,2);
call tablekey_insert($[schemaid],$[table2id],'TableId','TableId',true,getKeyTypeId('INTEGER'),0,1);
call tablekey_insert($[schemaid],$[table2id],'Name','Name',false,getKeyTypeId('VARCHAR'),300,2);
call tablekey_insert($[schemaid],$[table2id],'TableIdFk','TableIdFk',false,getKeyTypeId('INTEGER'),0,3);
--
call relationship_insert($[schemaid],$[table1id],$[table2id],getRelationshipTypeId('Identifying'));
--
WbVarDef table1keyname="'TableId'";
WbVarDef table2keyname="'TableId'";
WbVarDef table1keyid=@"call getKeyId($[schemaid],$[table1id],$[table1keyname])";
WbVarDef table2keyid=@"call getKeyId($[schemaid],$[table2id],$[table2keyname])";
--
call relationshipkeypair_insert($[schemaid],$[table1id],$[table2id],getRelationshipTypeId('Identifying'),$[table1keyid],$[table2keyid]);
--
--Select
call keytype_select();
call relationshiptype_select();
call databaseschema_select();
call databasetable_select($[schemaid]);
call tablekey_select($[schemaid],$[table1id]);
call tablekey_select_pk($[schemaid],$[table1id]);
call tablekey_select_npk($[schemaid],$[table1id]);
call tablekey_select($[schemaid],$[table2id]);
call tablekey_select_pk($[schemaid],$[table2id]);
call tablekey_select_npk($[schemaid],$[table2id]);
call relationship_select($[schemaid]);
call relationshipkeypair_select($[schemaid],$[table1id],$[table2id],getRelationshipTypeId('Identifying'));
